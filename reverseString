public class reverseString {
	public static void main(String args[]) {
		Scanner s=new Scanner(System.in);
		String s1=s.next();
		String arr[]=s1.split(".");
		String s2=new String();
		//int n=s1.length;
		for(int i=arr.length-1;i>=0;i--)
		{
			System.out.print(arr[i]);
			if(i!=0)
				System.out.print(".");
		}
			
		}
		
		
	}


